{
  "name": "@sammons/code-outline-parser",
  "version": "2.1.0",
  "description": "Tree-sitter based parser for JavaScript/TypeScript code analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --config ../../vitest.config.ts src",
    "test:watch": "vitest --config ../../vitest.config.ts src"
  },
  "keywords": [
    "tree-sitter",
    "parser",
    "typescript",
    "javascript",
    "ast"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "dependencies": {
    "tree-sitter": "0.21.1",
    "tree-sitter-javascript": "0.21.4",
    "tree-sitter-typescript": "0.23.2"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "typescript": "5.9.2"
  }
}
