{
  "name": "@sammons/code-outline-cli",
  "version": "2.1.0",
  "description": "CLI tool to parse and provide concise output for JavaScript/TypeScript files",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "bin": {
    "code-outline": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest run --config ../../vitest.config.ts src",
    "test:watch": "vitest --config ../../vitest.config.ts src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "cli",
    "tree-sitter",
    "parser",
    "typescript",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@sammons/code-outline-parser": "workspace:*",
    "@sammons/code-outline-formatter": "workspace:*",
    "fast-glob": "3.3.3"
  },
  "devDependencies": {
    "@types/node": "24.5.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  }
}
